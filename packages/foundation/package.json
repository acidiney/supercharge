    "@supercharge/contracts": "~0.0.0",
    "@supercharge/goodies": "~1.4.0"
  },
  "devDependencies": {
    "@hapi/code": "~8.0.1",
    "@hapi/lab": "~22.0.4",
    "typescript": "~3.9.5"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist",
    "index.js"
  ],
